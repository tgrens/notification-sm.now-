        <value>(function(outputs, steps, params, stepResult, assertEqual) {&#13;
	let record = new GlideRecord('incident');&#13;
	record.newRecord();&#13;
	record.setValue ('category', 'inquiry');&#13;
&#13;
	const field = 'category';&#13;
	let languageId = 'en';&#13;
&#13;
	const helper = new FieldTranslatorProcessor();&#13;
	const result = helper.translate(record, field, languageId);&#13;
&#13;
	assertEqual({&#13;
		name: 'FieldTranslatorProcessor#translate()',&#13;
		value: result,&#13;
		shouldbe: 'Inquiry / Help'&#13;
	});&#13;
&#13;
&#13;
	//Rerun function call with different languageId&#13;
	languageId = 'de';&#13;
	const resultDE = helper.translate(record, field, languageId);&#13;
		assertEqual({&#13;
		name: 'FieldTranslatorProcessor#translateFieldValue()',&#13;
		value: resultDE,&#13;
		shouldbe: 'Anforderung/Hilfe'&#13;
	})&#13;
&#13;
})(outputs, steps, params, stepResult, assertEqual);&#13;
</value>
