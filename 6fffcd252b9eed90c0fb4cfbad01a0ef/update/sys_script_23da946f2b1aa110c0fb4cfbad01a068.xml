        <script><![CDATA[/* eslint-disable camelcase */
/* eslint-disable no-undef */
// eslint-disable-next-line no-unused-vars
(function executeRule(current, previous /* null when async */) {
    let notification = current.notification.toString();
    let query = QueryBuilder.findEntries('notification.sys_id', notification);
    let entry = x_dtitg_core_smnow.GlideRecordUtils.hasEntry(Constants.CONFIGURATION, query);

    if (entry) {
        let message = gs.getMessage('Configuration already exists');
        gs.addErrorMessage(message);

        current.setAbortAction(true);
    }
}(current, previous));
]]></script>
